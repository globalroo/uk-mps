{
  "name": "uk-mps",
  "license": "MIT",
  "version": "0.2.0",
  "private": false,
  "homepage": "https://globalroo.github.io/uk-mps/",
  "dependencies": {
    "@babel/core": "7.0.0-beta.34",
    "autoprefixer": "7.1.6",
    "axios": "0.17.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-module-resolver": "3.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "bluebird": "3.5.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "codecov": "3.0.0",
    "css-loader": "0.28.7",
    "date-fns": "1.29.0",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.5.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "gh-pages": "1.1.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "lodash": "4.17.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "1.9.2",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-test-renderer": "16.2.0",
    "semantic-ui-css": "2.2.12",
    "semantic-ui-react": "0.77.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "deploy": "gh-pages -d build",
    "predeploy": "yarn run build",
    "build": "node scripts/build.js",
    "lint": "eslint 'src/**/*js' --fix && eslint 'test/**/*js' --fix",
    "prettier": "prettier --write 'src/**/*.js' && prettier --write 'test/**/*.js' && yarn lint",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "testcov": "node scripts/test.js --env=jsdom --coverage && codecov",
    "package": "rimraf coverage && rimraf node_modules"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.34",
    "@babel/preset-env": "7.0.0-beta.34",
    "babel-plugin-lodash": "3.3.2",
    "jest-css-modules": "1.1.0"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "coveragePathIgnorePatterns": [
      "src/registerServiceWorker.js",
      "src/index.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/test/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
